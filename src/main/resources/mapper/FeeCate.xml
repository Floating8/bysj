<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--suppress ALL -->
<mapper namespace="com.pzhu.bysj.dao.FeeCateDao">

    <select id="findFee" resultType="java.lang.String">
        select feecate.remark from feecate where id = #{id}
    </select>
    <select id="findFeeId" resultType="java.lang.Integer">
        select id from feecate where remark = #{remark}
    </select>
</mapper>